<style>
    /* navbar */
    * {
        margin: 0%;
        padding: 0px;
    }

    ul {
        display: flex;
        align-items: center;
        height: 100px;
    }

    li {
        list-style: none;
        margin: auto;
        text-align: center;
    }

    a {
        font-size: 30px;
        font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        text-decoration: none;
        color: #551A8B;
    }

    a:hover {
        text-decoration: underline;
    }

    button {
        border: 0px solid;
        background-color: #ed1c94;
        border-radius: 25px;
        padding: 15px;
    }

    .dropbtn {
        border: none;
    }

    .dropdown {
        position: relative;
        display: inline-block;
    }

    .dropdown-content {
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
        z-index: 1;
    }

    .dropdown-content a {
        color: black;
        padding: 12px 16px;
        text-decoration: none;
        display: block;
    }

    .dropdown-content a:hover {
        background-color: #ddd;
    }

    .dropdown:hover .dropdown-content {
        display: block;
    }

    /* footer */
    h1 {
        text-align: center;
        font-size: 30px;
        color: white;
        margin: 0px 0px 20px 0px;
    }

    .secondul a {
        color: white;
        font-size: 22px;
    }

    .secondul a:hover {
        color: rgb(93, 93, 234);
        text-decoration: none;
    }

    .last {
        display: flex;
        flex-direction: column;
        color: white;
        font-size: 23px;
        margin-top: 80px;
        width: 100px;
        height: fit-content;
    }

    .last li {
        margin: 5px;
        text-align: left;
    }
</style>

